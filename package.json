{
  "name": "kahootbot",
  "version": "2.0.0-beta",
  "description": "A simple Kahoot Bot that answer randomly",
  "author": "Ilingu",
  "license": "MIT",
  "keywords": [
    "kahoot",
    "bot",
    "puppeteer",
    "fastify"
  ],
  "main": "build/app.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ilingu/kahoot-bot"
