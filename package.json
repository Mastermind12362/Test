    "socket.io": "^4.5.1"
  },
  "devDependencies": {
    "@tsconfig/node16-strictest-esm": "^1.0.1",
    "@types/node": "^17.0.39",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.7.3"
  }
